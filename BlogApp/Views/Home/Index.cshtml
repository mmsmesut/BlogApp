@using BlogApp.Data.Models
@model List<Article>

@{
    ViewBag.Title = "Index";
}


@{

    Html.RenderPartial("ArticleListPartial", Model);
}


@section Arama {
    @{
        Html.RenderAction("SearchPartial", "Search");
    }
}

@section Kategoriler{
    @{
        Html.RenderAction("KategoriPartial", "Category");
    }
}

@section Etiketler{
