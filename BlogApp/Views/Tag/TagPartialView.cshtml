
@using BlogApp.Data.Models
@model IEnumerable<Tag>

<div class="sidebox widget">
    <h3 class="widget-title section-title">Etiketler</h3>
    <ul class="tag-list">
        @foreach (Tag tag in Model)
        {
            <li><a href="/Tag/Index/@tag.TagId">@tag.Adi (@tag.Articles.Count)</a></li>

        }
    </ul>
</div>
